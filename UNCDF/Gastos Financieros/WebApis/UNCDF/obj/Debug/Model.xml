		<Element Type="SqlProcedure" Name="[dbo].[sp_User_List]">
			<Property Name="BodyScript">
				<Value><![CDATA[
	SELECT 
			UserId,
			Type,
			u.[User],
			Password,
			Name,
			Status
	FROM	[User] u
	WHERE	(u.[User] like '%'+@IUser+'%' or @IUser = '')
	AND		(Name like '%'+@IName+'%' or @IName = '')
	AND		u.UserId NOT IN (select onguserId from OngUser)
RETURN 0]]></Value>
